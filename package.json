    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.56.0",
    "eslint-config-galexia": "https://github.com/Galexia-Agency/eslint-config.git",
    "stylelint": "^15.11.0",
    "stylelint-config-galexia": "https://github.com/Galexia-Agency/stylelint-config.git"
  },
  "browserslist": [
    "last 5 versions and not ie <= 11 and > 0.2% and not dead and not op_mini all"
