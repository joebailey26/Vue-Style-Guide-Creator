{
  "name": "vue-style-guide-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint:scripts": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix:scripts": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:styles": "stylelint \"**/*.{vue,scss}\"",
    "fix:styles": "stylelint \"**/*.{vue,scss}\" --fix"
  },
  "dependencies": {
    "font-picker-vue": "^1.0.3",
    "vue": "^3.0.0",
    "vue-color": "^2.8.1",
    "vue-js-modal": "^2.0.1",
    "vue-template-compiler": "^2.7.15"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.5.19",
    "eslint": "^7.29.0",
    "eslint-config-galexia": "https://github.com/Galexia-Agency/eslint-config",
    "stylelint": "^15.11.0",
    "stylelint-config-galexia": "https://github.com/Galexia-Agency/stylelint-config"
  },
  "optionalDependencies": {
    "typescript": "^4.9.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 5 versions and not ie <= 11 and > 0.2% and not dead and not op_mini all"
  ],
  "engines": {
    "node": "^18.12.0",
    "yarn": "^1.10"
  },
  "packageManager": "yarn@3.6.3"
}
